{
  "name": "cucumber-bdd",
  "version": "1.0.0",
  "description": "this is automation of pw with bdd",
  "main": "index.js",
  "scripts": {
    "test": "cucumber-js test & npx ts-node index.ts",
    "test-parallel": "cucumber-js features/create.wko.feature --parallel 2 & npx ts-node src/helper/index.ts",
    "sanity": "<----------------sanity Suite------------------------------------->",
    "wko-e2e-test": "cucumber-js features/**/*.feature --tags @sanityWorkOrder & npx ts-node src/helper/index.ts",
    "wkoListView-e2e": "cucumber-js features/ListView.feature --tags @sanityWKListView & npx ts-node src/helper/index.ts",
    "mr-e2e-test": "cucumber-js features/MaintenanceRecords.feature --tags @sanityMR & npx ts-node src/helper/index.ts",
    "por-e2e-test": "cucumber-js features/PorRequisition.feature --tags @sanityPOR & npx ts-node src/helper/index.ts",

    "div1": "-------------------------Work Order Records-------------------------------------------------",
    "wko-test": "cucumber-js features/wko.feature --tags @wko & npx ts-node src/helper/index.ts",
    "wko-close-wko-test": "cucumber-js features/close.wko.feature --tags @closeWko & npx ts-node src/helper/index.ts",
    "wko-cancel-wko-test": "cucumber-js features/cancel.wko.feature --tags @cancelWko & npx ts-node src/helper/index.ts",
    "wko-hold-wko-test": "cucumber-js features/hold.wko.feature --tags @holdWko & npx ts-node src/helper/index.ts",
    "wko-delete-wko-test": "cucumber-js features/workOrder.Delete.feature --tags @deleteWko & npx ts-node src/helper/index.ts",
    "div2": "-------------------------Work Order Records List View-------------------------------------------------",
    "wkoListView-create-test": "cucumber-js features/ListView.Create.ListView.feature --tags @closeListViewWko & npx ts-node src/helper/index.ts",
    "wkoListView-update-wko-test": "cucumber-js features/ListView.Search.Update.feature --tags @updateWko & npx ts-node src/helper/index.ts",
    "wkoListView-sort-test": "cucumber-js features/ListView.Sorting.feature --tags @sortWko & npx ts-node src/helper/index.ts",
    "wkoListView-filter-column-row": "cucumber-js features/ListView.Filter.feature --tags @filterwko & npx ts-node src/helper/index.ts",
    "wkoListView-omit-lookup": "cucumber-js features/ListView.OmitAndLookup.feature --tags @omitAndLookup & npx ts-node src/helper/index.ts",
    "wkoListView-layout-with-grouping": "cucumber-js features/ListView.Layout.WithGroup.feature --tags @withgroup & npx ts-node src/helper/index.ts",
    "wkoListView-layout-without-grouping": "cucumber-js features/ListView.Layout.WithoutGroup.feature --tags @withoutgroup & npx ts-node src/helper/index.ts",
    "wkoListView-language-label-change": "cucumber-js features/ListView.Label.Change.Language.feature --tags @changeLabel & npx ts-node src/helper/index.ts",
    "wkoListView-maintenance-advisor": "cucumber-js features/ListView.Maintenance.Advisor.feature --tags @setLayoutMA & npx ts-node src/helper/index.ts",
    "div3": "-------------------------Maintenance Request Records-------------------------------------------------",
    "mr-create-close": "cucumber-js features/MaintenanceRecords.Create.Close.feature --tags @convertToWorkOrder & npx ts-node src/helper/index.ts",
    "mr-auto-convert": "cucumber-js features/MaintenanceRecords.AutoConvert.feature --tags @mrAutoConvert & npx ts-node src/helper/index.ts",
    "mr-list-view": "cucumber-js features/MaintenanceRecords.ListView.feature --tags @mrListView & npx ts-node src/helper/index.ts",
    "mr-delete-test": "cucumber-js features/MaintenanceRecords.Delete.feature --tags @deleteMrRecord & npx ts-node src/helper/index.ts",
    "mr-approval-flow": "cucumber-js features/MaintenanceRecords.Approval.Flow.feature --tags @replyToRequester & npx ts-node src/helper/index.ts",
    "div4": "-------------------------Purchase Order Requisition Records-------------------------------------------------",
    "por-create-requisition": "cucumber-js features/PorRequisition.Create.feature --tags @createPOR & npx ts-node src/helper/index.ts",
    "listView-por-requisition": "cucumber-js features/PorRequisition.ListView.feature --tags @createPORListView & npx ts-node src/helper/index.ts",
    "div5": "-------------------------Cycle Count Records-------------------------------------------------",
    "cyc-create-convert-close": "cucumber-js features/cycle-count-records/CYC-Create.Convert.Close.feature --tags @createCycleCount & npx ts-node src/helper/index.ts"
  },
  "keywords": [
    "pw",
    "ts",
    "js"
  ],
  "author": "Shrikant Khairnar",
  "license": "ISC",
  "devDependencies": {
    "@cucumber/cucumber": "^10.9.0",
    "@playwright/test": "^1.40.1",
    "cross-env": "^7.0.3",
    "cucumber-html-reporter": "^7.1.1",
    "dotenv": "^16.3.2",
    "playwright": "^1.40.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@faker-js/faker": "^9.9.0",
    "winston": "^3.17.0",
    "xlsx": "^0.18.5"
  }
}
